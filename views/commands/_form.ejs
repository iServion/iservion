<form id="form-group" method="post" action="" class="boxy" enctype="multipart/form-data" [[[NO_VALI_DATE_NOVALIDATE]]]  >
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
    <div class="page-header">

    </div>

	<div class="row">
		<div class="col-md-6">
			<%- UI.Form.group("server_id", zForms.label["server_id"], zForms.fn("server_id",{tabindex:1})); %>
			<%- UI.Form.group("service_id", zForms.label["service_id"], zForms.fn("service_id",{tabindex:2})); %>
			<%- UI.Form.group("title", zForms.label["title"], zForms.fn("title",{tabindex:6})); %>
			<%- UI.Form.group("command", zForms.label["command"], zForms.fn("command",{tabindex:3})); %>
			<%- UI.Form.group("long_command", zForms.label["long_command"], zForms.fn("long_command",{tabindex:8})); %>
		</div>
		<div class="col-md-6">
			<%- UI.Form.group("description", zForms.label["description"], zForms.fn("description",{tabindex:4})); %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
		</div>
	</div>


    <div class="row">
        <div class="col-md-12">
            <br>
            <%- UI.Form.submit() %>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <br>
            <p class="text-muted"><strong>*</strong> <%- LANGUAGE['required_not_empty'] %></p>
        </div>
    </div>

</form>